  "egg": {
    "declarations": true
  },
  "dependencies": {
    "@vercel/node": "^2.9.12",
    "egg": "^3",
    "egg-scripts": "^2",
    "egg-sequelize": "^6.0.0",
    "mysql2": "^3.1.2"
  },
  "devDependencies": {
    "egg-bin": "^5",
    "egg-ci": "^2",
    "egg-mock": "^5",
    "eslint": "^8",
    "eslint-config-egg": "^12",
